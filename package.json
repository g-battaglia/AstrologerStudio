{
  "name": "frontend.astrologerstudio.com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "bun run db:generate && next build",
    "start": "next start",
    "lint": "eslint . --ext ts,tsx,js,jsx --max-warnings 100",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:generate": "bun prisma generate",
    "db:migrate": "bun prisma migrate dev",
    "db:deploy": "bun prisma migrate deploy",
    "db:studio": "bun prisma studio",
    "db:setup": "bun install && bun run db:generate && bun run db:migrate",
    "db:format": "bun prisma format",
    "user:create": "bun run ./scripts/create-user.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
